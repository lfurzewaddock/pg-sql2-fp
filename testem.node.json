{
  "src_files": [
    "src/**/*.js",
    "test/**/*.test.js"
  ],
  "framework": "tap",
  "launchers": {
    "Node": {
      "command": "NODE_ENV=development node_modules/.bin/tape --require esm test/jsdom.environment.js test/{,**/}/*.test.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": ["Node"]
}